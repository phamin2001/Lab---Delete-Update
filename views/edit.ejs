    <body>
        <form action="/dog/<%=id%>?_method=PUT" method="POST">
            Name: <input type="text" name="name" value="<%=singleDog.name%>"/><br/>
            Age: <input type="text" name="age" value="<%=singleDog.age%>"/><br/>
            Is Eat Meet:
            <input type="checkbox" name="eatMeet" 
            <% if(singleDog.eatMeet === true){ %>
             checked=<%=true%>
              <% }  %> 
              />
              <button type="submit" value="submit change">Submit Change</button>
        </form>

        <form action="/dog/<%=id%>?_method=DELETE" method="POST">
                <div><button type="submit">Delete</button></div>
        </form>

    </body>
